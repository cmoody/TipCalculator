<?xml version='1.0' encoding='utf-8'?>
<widget id="com.chasemood.TipCalculator" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>TipCalculator</name>
    <description>
        Backbone Calculator
    </description>
    <author email="chase.moody@mutualmobile.com" href="http://mutualmobile.com">
        Chase Moody
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="false" />
		<preference name="ShowSplashScreenSpinner" value="false" />
		<preference name="AutoHideSplashScreen" value="false" />
		<preference name="DisallowOverscroll" value="true" />
		<preference name="SuppressesIncrementalRendering" value="true" />
</widget>
